    "module": "../../../../server/passport-bdoor",
    "usernameField": "email",
    "passwordField": "password",
    "authPath": "/auth/signin/bdoor",
    "successRedirect": "/auth/signin/success",
    "failureRedirect": "/auth/signin/failure",
    "failureFlash": false
  }
}
